{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watchAll",
    "test:update": "react-scripts test --watchAll --updateSnapshot",
    "test:coverage": "cross-env CI=true react-scripts test --coverage",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:js": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "lint:js:fix": "eslint './src/**/*.{js,jsx,ts,tsx}' --fix",
    "lint:styles": "stylelint '**/*.scss' --syntax scss",
    "format": "prettier-eslint \"src/**/*.tsx\"",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public --ci",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@dump247/storybook-state": "1.6.1",
    "@emotion/core": "^10.0.22",
    "@material-ui/core": "^4.7.1",
    "@material-ui/icons": "^4.5.1",
    "@redux-saga/core": "1.0.3",
    "@types/classnames": "2.2.9",
    "@types/color": "^3.0.1",
    "@types/http-proxy-middleware": "0.19.3",
    "@types/lodash": "^4.14.138",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "7.1.2",
    "@types/react-router-dom": "4.3.5",
    "@types/redux-mock-store": "1.0.1",
    "@types/yup": "0.26.23",
    "axios": "0.19.0",
    "classnames": "2.2.6",
    "color": "^3.1.2",
    "connected-react-router": "^6.6.1",
    "cross-env": "5.2.1",
    "emotion": "^10.0.23",
    "emotion-theming": "^10.0.19",
    "formik": "1.5.8",
    "history": "^4.10.1",
    "husky": "3.0.5",
    "lodash": "^4.17.15",
    "moment": "2.24.0",
    "polished": "3.4.1",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-jss": "^10.0.0",
    "react-measure": "2.3.0",
    "react-popper": "^1.3.7",
    "react-redux": "^7.1.3",
    "react-router": "5.0.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-select": "^3.0.4",
    "reactstrap": "8.0.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-mock-store": "1.5.3",
    "redux-saga": "^1.1.3",
    "redux-saga-routines": "^3.2.1",
    "redux-thunk": "^2.3.0",
    "reselect": "4.0.0",
    "typescript": "3.5.3",
    "utility-types": "^3.10.0",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@storybook/addon-a11y": "5.1.11",
    "@storybook/addon-actions": "5.1.11",
    "@storybook/addon-backgrounds": "5.1.11",
    "@storybook/addon-info": "5.1.11",
    "@storybook/addon-knobs": "5.1.11",
    "@storybook/addon-links": "5.1.11",
    "@storybook/addon-viewport": "5.1.11",
    "@storybook/addons": "5.1.11",
    "@storybook/react": "5.1.11",
    "@types/enzyme": "3.10.3",
    "@types/react-input-autosize": "2.0.1",
    "@types/react-select": "^3.0.4",
    "@types/redux-actions": "2.6.1",
    "@types/redux-saga-routines": "3.1.0",
    "@types/storybook__react": "4.0.2",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-loader": "8.0.6",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-typescript": "4.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "5.0.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest-enzyme": "7.1.0",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "3.2.0",
    "redux-saga-test-plan": "4.0.0-beta.4",
    "stylelint": "10.1.0",
    "stylelint-config-recommended": "2.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 3 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 64,
        "branches": 54,
        "functions": 53,
        "lines": 66
      }
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/**/*{stories,style}*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:js && npm run test:coverage"
    }
  }
}
